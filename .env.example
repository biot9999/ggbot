# Telegram Bot Configuration
BOT_TOKEN=your_bot_token_here

# Telegram API Credentials (get from https://my.telegram.org)
API_ID=your_api_id
API_HASH=your_api_hash

# Admin Configuration (comma-separated Telegram user IDs)
ADMIN_IDS=123456789,987654321

# Rate Limiting
MESSAGE_DELAY_MIN=5
MESSAGE_DELAY_MAX=15
ACCOUNT_SWITCH_DELAY=30
MAX_CONCURRENT_TASKS=3
MESSAGES_PER_ACCOUNT=50

# Proxy Testing
PROXY_TEST_URL=https://api.telegram.org

# Paths
SESSION_DIR=data/sessions
PROXY_FILE=data/proxies/proxies.json
TARGET_DIR=data/targets
BLACKLIST_FILE=data/targets/blacklist.txt
LOG_DIR=logs
